class Solution {
    public String reverseWords(String s) {
        String arr[]=s.split("\\s+");
        StringBuilder brr=new StringBuilder();
        int i;
        for(i=arr.length-1;i>=0;--i)
        {
            brr.append(arr[i] + " ");
        }
        return brr.toString().trim();
        
    }
}
